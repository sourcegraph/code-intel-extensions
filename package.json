{
  "$schema": "https://raw.githubusercontent.com/sourcegraph/sourcegraph/master/shared/src/schema/extension.schema.json",
  "name": "code-intel-extensions",
  "description": "Sourcegraph code intelligence extensions",
  "private": true,
  "sideEffects": false,
  "publisher": "sourcegraph",
  "version": "0.0.0-DEVELOPMENT",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sourcegraph/code-intel-extensions"
  },
  "scripts": {
    "prettier": "prettier --write --list-different '**/*.{ts,js?(on),md,yml}'",
    "prettier-check": "yarn run prettier --write=false",
    "tslint": "tslint -p tsconfig.json --format stylish",
    "eslint": "eslint --cache '**/*.ts?(x)'",
    "test": "nyc --reporter=text mocha '**/*.test.ts'",
    "deduplicate": "yarn-deduplicate -s fewer",
    "generate": "yarn -s && yarn run ts-node -P tsconfig.json ./dev/scripts/generate.ts",
    "publish": "yarn run ts-node -P tsconfig.json ./dev/scripts/publish.ts"
  },
  "dependencies": {
    "@sourcegraph/lightstep-tracer-webworker": "^0.20.14-webworker.2",
    "@sourcegraph/typescript-language-server": "^0.3.7-fork",
    "@sourcegraph/vscode-ws-jsonrpc": "0.0.3-fork",
    "delay": "^4.3.0",
    "fs-extra": "^8.1.0",
    "ix": "^3.0.0",
    "lodash": "^4.17.15",
    "mz": "^2.7.0",
    "opentracing": "^0.14.4",
    "path-browserify": "^1.0.0",
    "rxjs": "^6.5.4",
    "semver": "^7.1.3",
    "sourcegraph": "^23.1.0",
    "tagged-template-noop": "^2.1.1",
    "type-zoo": "^3.4.1",
    "typescript": "^3.7.5",
    "uuid": "^3.4.0",
    "vscode-languageserver-types": "^3.15.1",
    "whatwg-url": "^8.0.0",
    "yargs": "^15.1.0",
    "yarn": "^1.22.0"
  },
  "devDependencies": {
    "@sourcegraph/eslint-config": "^0.7.1",
    "@sourcegraph/extension-api-stubs": "^0.2.3",
    "@sourcegraph/prettierrc": "^3.0.2",
    "@sourcegraph/tsconfig": "^4.0.0",
    "@sourcegraph/tslint-config": "^13.4.0",
    "@types/fs-extra": "8.1.0",
    "@types/lodash": "4.14.149",
    "@types/mocha": "7.0.1",
    "@types/mock-require": "2.0.0",
    "@types/mz": "2.7.0",
    "@types/node": "13.7.4",
    "@types/semver": "7.1.0",
    "@types/uuid": "3.4.7",
    "@types/whatwg-url": "6.4.0",
    "@types/yargs": "15.0.3",
    "dot-json": "^1.2.0",
    "eslint": "^6.8.0",
    "json-schema-to-typescript": "^8.0.1",
    "lnfs-cli": "^2.1.0",
    "mkdirp": "^1.0.3",
    "mocha": "^7.0.1",
    "mock-require": "^3.0.3",
    "nyc": "^15.0.0",
    "parcel": "1.x",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5",
    "yarn-deduplicate": "^1.1.1"
  },
  "resolutions": {
    "vscode-languageserver-protocol": "3.14.1"
  }
}
